v00000000028e5b00_0 .net "rst", 0 0, v00000000029415f0_0;  1 drivers
E_00000000028e8e00 .event posedge, v00000000028ebc20_0;
    .scope S_00000000028e57a0;
T_0 ;
    %wait E_00000000028e8e00;
    %load/vec4 v00000000028e5b00_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v00000000028e5920_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v00000000028e5a60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v00000000028e59c0_0;
    %assign/vec4 v00000000028e5920_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v00000000028e5920_0;
    %addi 1, 0, 8;
    %assign/vec4 v00000000028e5920_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00000000028ea560;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0000000002940b60_0;
    %inv;
    %store/vec4 v0000000002940b60_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00000000028ea560;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000000002940b60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000029415f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000000002940d40_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0000000002940ca0_0, 0, 8;
    %delay 12, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000029415f0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0000000002940d40_0, 0, 1;
    %pushi/vec4 50, 0, 8;
    %store/vec4 v0000000002940ca0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0000000002940d40_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0000000002940d40_0, 0, 1;
    %pushi/vec4 200, 0, 8;
    %store/vec4 v0000000002940ca0_0, 0, 8;
    %delay 10, 0;
