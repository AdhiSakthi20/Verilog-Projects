#! c:/iverilog-x64/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1_1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000028ca540 .scope module, "counter_basic_tb" "counter_basic_tb" 2 2;
 .timescale -9 -12;
v000000000291e790_0 .var "clk", 0 0;
v000000000291e830_0 .net "count", 7 0, L_00000000028c8260;  1 drivers
v000000000291e8d0_0 .var "rst", 0 0;
S_00000000028ca6c0 .scope module, "uut" "counter_basic" 2 9, 3 1 0, S_00000000028ca540;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /OUTPUT 8 "count"
L_00000000028c8260 .functor BUFZ 8, v000000000291e650_0, C4<00000000>, C4<00000000>, C4<00000000>;
v00000000028c81c0_0 .net "clk", 0 0, v000000000291e790_0;  1 drivers
v000000000291e5b0_0 .net "count", 7 0, L_00000000028c8260;  alias, 1 drivers
v000000000291e650_0 .var "count_temp", 7 0;
v000000000291e6f0_0 .net "rst", 0 0, v000000000291e8d0_0;  1 drivers
E_00000000010dc050 .event posedge, v00000000028c81c0_0;
    .scope S_00000000028ca6c0;
T_0 ;
    %wait E_00000000010dc050;
    %load/vec4 v000000000291e6f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v000000000291e650_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000000000291e650_0;
    %addi 1, 0, 8;
    %assign/vec4 v000000000291e650_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00000000028ca540;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v000000000291e790_0;
    %inv;
    %store/vec4 v000000000291e790_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00000000028ca540;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000291e790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000291e8d0_0, 0, 1;
    %vpi_call 2 24 "$display", "Applying reset..." {0 0 0};
    %delay 12000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000000000291e8d0_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 31 "$display", "Resetting again..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000291e8d0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000000000291e8d0_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_00000000028ca540;
T_3 ;
    %vpi_call 2 43 "$monitor", "Time=%0t | rst=%b | count=%d", $time, v000000000291e8d0_0, v000000000291e830_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_00000000028ca540;
T_4 ;
    %vpi_call 2 48 "$dumpfile", "counter_tb.vcd" {0 0 0};
    %vpi_call 2 49 "$dumpvars", 32'sb00000000000000000000000000000000, S_00000000028ca540 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
