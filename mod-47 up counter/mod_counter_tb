v000000000290df70_0 .net "rst", 0 0, v000000000290e1f0_0;  1 drivers
E_000000000093bf90 .event posedge, v00000000028b8dc0_0;
    .scope S_00000000028b9f00;
T_0 ;
    %wait E_000000000093bf90;
    %load/vec4 v000000000290df70_0;
    %nor/r;
    %pushi/vec4 46, 0, 8;
    %load/vec4 v00000000028ba120_0;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %or;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v00000000028ba120_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v00000000028ba120_0;
    %addi 1, 0, 8;
    %assign/vec4 v00000000028ba120_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00000000028b8c40;
T_1 ;
    %delay 5, 0;
    %load/vec4 v000000000290e010_0;
    %inv;
    %store/vec4 v000000000290e010_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_00000000028b8c40;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000290e010_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000290e1f0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v000000000290e150_0, 0, 8;
    %delay 12, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000000000290e1f0_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000000000290e1f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000000000290e1f0_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
