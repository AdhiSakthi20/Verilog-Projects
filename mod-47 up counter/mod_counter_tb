L_00000000028b8e60 .functor BUFZ 8, v00000000028ba120_0, C4<00000000>, C4<00000000>, C4<00000000>;
v00000000028b8dc0_0 .net "clk", 0 0, v000000000290e010_0;  1 drivers
v00000000028ba080_0 .net "count", 7 0, L_00000000028b8e60;  alias, 1 drivers
v00000000028ba120_0 .var "count_temp", 7 0;
